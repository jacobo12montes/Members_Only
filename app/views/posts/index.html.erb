
<div class='post-index'>
<h1>All Posts</h1>

<ul id="posts">
  <% @posts.each do |post| %>
    <li>
    <label for = 'title'>Post Title:</label>  <%= link_to post.title, post_path(post) %>
  </li>
  <% end %>
 </ul>

<% if member_signed_in? %>
<%= link_to "Create a New Post", new_post_path, class: "new_post" %> <br>
<%= current_member.name %>

  <%= button_to "logout", destroy_member_session_path, method: :delete %>
<% else %>
<%= button_to "sign_up", new_member_registration_path %>
<%= button_to "login", new_member_session_path %>


<%end%>

</div>




